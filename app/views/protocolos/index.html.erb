<p id="notice"><%= notice %></p>

<h1>Listing Protocolos</h1>

<table>
  <thead>
    <tr>
      <th>Nome</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @protocolos.each do |protocolo| %>
      <tr>
        <td><%= protocolo.nome %></td>
        <td><%= link_to 'Show', protocolo %></td>
        <td><%= link_to 'Edit', edit_protocolo_path(protocolo) %></td>
        <td><%= link_to 'Destroy', protocolo, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Protocolo', new_protocolo_path %>
